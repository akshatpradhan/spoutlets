<h3>Your Backstory</h3>
<div class="row">
  <div class="span12">
    <div class="hero-unit">
      <h4>What's been on your mind over time</h4>
      <p>Graph placeholder</p>
    </div>
  </div>

  <div class="span9">
    <!-- Entry -->
    <% @entries.each do |entry| %>
    <div class="well">
      <div class="row">
        <div class="span1">
          <span class="label"><%= entry.category %> </span>
        </div>
        <div class="span5">
          <br />
          <p><small><%= entry.content %></small></p>
          <ul class="nav nav-list">
            <li class="divider"></li>
          </ul>
        </div>
        <div class="span1">
          <br />
          <span class="muted"><small><%= Date.today.strftime('%a %b %d, %Y') %></small></span>
        </div>
        <div class="span1">
          <span class="badge badge-info">14</span> <br />
          <span class="text-info"> been there</span>
        </div>
      </div>

      <div class="row">
        <div class="span1 offset1">
          <p class="muted"><small>happiness</small></p>
          <div class="progress">
            <div class="bar" style="width: 20%;"></div>
          </div>
        </div>
        <div class="span1">
          <p class="muted"><small>anxiety</small></p>
          <div class="progress">
            <div class="bar" style="width: 80%;"></div>
          </div>
        </div>
        <div class="span1">
          <p class="muted"><small>irritation</small></p>
          <div class="progress">
            <div class="bar" style="width: 40%;"></div>
          </div>
        </div>
        <div class="span4">
          <button class="btn btn-mini pull-right"
                  type="button"><i class="icon-ok"></i> Share with therapist</button>
        </div>
      </div>
    </div>
    <% end %><!-- end of Entry -->
  </div>
</div>
